#ifndef __$__daoexception__
#define __$__daoexception__ 1234
#include<iostream>
using namespace std;
namespace inventory
{
namespace business_layer
{
class BLException:public exception
{
string message;
public:
BLException();
BLException(string message);
BLException(const BLException &other);
virtual ~BLException() throw();
BLOException & operator
=(const BLException &other);
const char * what() const throw();
};
}
}
#endif

